# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  snapp_ink_config = debug
endif
ifeq ($(config),release)
  snapp_ink_config = release
endif

PROJECTS := snapp_ink

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

snapp_ink:
ifneq (,$(snapp_ink_config))
	@echo "==== Building snapp_ink ($(snapp_ink_config)) ===="
	@${MAKE} --no-print-directory -C . -f snapp_ink.make config=$(snapp_ink_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f snapp_ink.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   snapp_ink"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"